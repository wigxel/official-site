-- Table Definition
CREATE TABLE "public"."posts" (
    "id" int4 NOT NULL DEFAULT nextval('posts_id_seq'::regclass),
    "title" varchar,
    "hero_image_id" int4,
    "content" jsonb,
    "meta_title" varchar,
    "meta_image_id" int4,
    "meta_description" varchar,
    "published_at" timestamptz(3),
    "slug" varchar,
    "slug_lock" bool DEFAULT true,
    "updated_at" timestamptz(3) NOT NULL DEFAULT now(),
    "created_at" timestamptz(3) NOT NULL DEFAULT now(),
    "_status" "public"."enum_posts_status" DEFAULT 'draft'::enum_posts_status,
    "post_type" "public"."enum_posts_post_type",
    "description" varchar,
    PRIMARY KEY ("id")
);

