    "_order" int4 NOT NULL,
    "_parent_id" int4 NOT NULL,
    "id" varchar NOT NULL,
    "email_to" varchar,
    "cc" varchar,
    "bcc" varchar,
    "reply_to" varchar,
    "email_from" varchar,
    "subject" varchar NOT NULL DEFAULT 'You''ve received a new message.'::character varying,
    "message" jsonb,
    PRIMARY KEY ("id")
);

