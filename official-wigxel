DROP TABLE IF EXISTS "public"."pages";
-- Sequence and defined type
CREATE SEQUENCE IF NOT EXISTS pages_id_seq;
DROP TYPE IF EXISTS "public"."enum_pages_hero_type";
CREATE TYPE "public"."enum_pages_hero_type" AS ENUM ('none', 'highImpact', 'mediumImpact', 'lowImpact');
DROP TYPE IF EXISTS "public"."enum_pages_status";
CREATE TYPE "public"."enum_pages_status" AS ENUM ('draft', 'published');

-- Table Definition
CREATE TABLE "public"."pages" (
    "id" int4 NOT NULL DEFAULT nextval('pages_id_seq'::regclass),
    "title" varchar,
    "hero_type" "public"."enum_pages_hero_type" DEFAULT 'lowImpact'::enum_pages_hero_type,
    "hero_rich_text" jsonb,
    "hero_media_id" int4,
    "meta_title" varchar,
    "meta_image_id" int4,
    "meta_description" varchar,
    "published_at" timestamptz(3),
    "slug" varchar,
    "slug_lock" bool DEFAULT true,
    "updated_at" timestamptz(3) NOT NULL DEFAULT now(),
    "created_at" timestamptz(3) NOT NULL DEFAULT now(),
    "_status" "public"."enum_pages_status" DEFAULT 'draft'::enum_pages_status,
    PRIMARY KEY ("id")
);

